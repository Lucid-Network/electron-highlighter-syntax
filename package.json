{
  "name": "autumn-highlighter-syntax",
  "theme": "syntax",
  "version": "2.2.2",
  "description": "A tweaked one-dark syntax with autumn colors and Spacegray background",
  "keywords": [
    "syntax",
    "theme",
    "electron",
    "dark",
    "autumn",
    "spacegray"
  ],
  "repository": "https://github.com/Lucid-Network/autumn-highlighter-syntax",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "devDependencies": {
    "lint-staged": "^7.0.5",
    "prettier": "1.12.1",
    "yorkie": "^1.0.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "styles/syntax/*.less": [
      "prettier --write",
      "git add"
    ]
  }
}
